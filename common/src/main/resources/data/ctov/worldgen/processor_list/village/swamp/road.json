{
    "processors": [
        {
            "processor_type": "lithostitched:condition",
            "random_mode": "per_block",
            "if_true": {
                "type": "lithostitched:all_of",
                "conditions": [
                    {
                        "type": "lithostitched:matching_blocks",
                        "blocks": "minecraft:cobblestone",
                        "match_type": "input"
                    }
                ]
            },
            "then": {
                "processor_type": "lithostitched:condition",
                "random_mode": "per_block",
                "if_true": {
                    "type": "lithostitched:matching_blocks",
                    "blocks": "minecraft:water",
                    "match_type": "location"
                },
                "then": {
                    "processor_type": "lithostitched:set_block",
                    "state_provider": {
                        "type": "minecraft:weighted_state_provider",
                        "entries": [
                            {
                                "weight": 1,
                                "data": {
                                    "Name": "minecraft:mangrove_planks"
                                }
                            },
                            {
                                "weight": 1,
                                "data": {
                                    "Name": "minecraft:campfire",
                                    "Properties": {
                                        "facing": "north",
                                        "lit": "false",
                                        "signal_fire": "false",
                                        "waterlogged": "true"
                                    }
                                }
                            }
                        ]
                    },
                    "preserve_state": false,
                    "random_mode": "per_block"
                },
                "else": {
                    "processor_type": "lithostitched:set_block",
                    "state_provider": {
                        "type": "minecraft:weighted_state_provider",
                        "entries": [
                            {
                                "weight": 1,
                                "data": {
                                    "Name": "minecraft:mud_bricks"
                                }
                            },
                            {
                                "weight": 5,
                                "data": {
                                    "Name": "minecraft:dirt_path"
                                }
                            },
                            {
                                "weight": 4,
                                "data": {
                                    "Name": "minecraft:grass_block"
                                }
                            },
                            {
                                "weight": 0,
                                "data": {
                                    "Name": "minecraft:mud"
                                }
                            }
                        ]
                    },
                    "random_mode": "per_block"
                }
            },
            "else": {
                "processor_type": "minecraft:nop"
            }
        }
    ]
}