{
	"processors": [
		{
			"processor_type": "lithostitched:condition",
			"random_mode": "per_block",
			"if_true": {
				"type": "lithostitched:all_of",
				"conditions": [
					{
						"type": "lithostitched:matching_blocks",
						"blocks": "minecraft:cobblestone",
						"match_type": "input"
					}
				]
			},
			"then": {
				"processor_type": "lithostitched:condition",
				"random_mode": "per_block",
				"if_true": {
					"type": "lithostitched:matching_blocks",
					"blocks": "minecraft:water",
					"match_type": "location"
				},
				"then": {
					"processor_type": "lithostitched:set_block",
					"state_provider": {
						"type": "minecraft:simple_state_provider",
						"state": {
							"Name": "minecraft:jungle_planks"
						}
					}
				},
				"else": {
					"processor_type": "lithostitched:set_block",
					"state_provider": {
						"type": "minecraft:weighted_state_provider",
						"entries": [
							{
								"weight": 1,
								"data": {
									"Name": "minecraft:sand"
								}
							},
							{
								"weight": 5,
								"data": {
									"Name": "minecraft:sandstone"
								}
							},
							{
								"weight": 3,
								"data": {
									"Name": "minecraft:smooth_sandstone"
								}
							}
						]
					},
					"random_mode": "per_block"
				}
			},
			"else": {
				"processor_type": "minecraft:nop"
			}
		}
	]
}